{
  "name": "leangoo Extrenal Window",
  "description": "leangoo External Window",
  "version": "0.1",
  "manifest_version": 2,

  "icons": {
    "128": "icon.png"
  },

  "app": {
    "background": {
      "scripts": [
        "background.js"
      ]
    }
  },

  "sockets": {
    "tcp": {
      "connect": "*:*"
    }
  },

  "permissions": [
    "webview",
    "alwaysOnTopWindows"
  ]
}
